	<queryString language="SQL">
		<![CDATA[select a.ID, a.NOME, a.ativo, a.ENDERECO, a.NUMERO, a.BAIRRO, c.DESCRICAO,
        a.CIDADE, a.ESTADO, a.CEP, a.EMAIL, a.TELEFONE, a.CELULAR, 
        a.CPF, a.RG, b.NOME as respNome, b.CPF as respCpf, b.RG as respRg, b.TELEFONE as respTel, b.CELULAR as respCelular,
        c.CODIGO, (c.MINUTOSSEMANAIS / 60) as horasSemanais, (c.MINUTOSSEMANAIS - ((c.MINUTOSSEMANAIS / 60)*60)) as minutosSemanais,
        (e.VALORAULAS / 12) as VALORMAXPARCELAS,
        c.QTDHORAS, c.DURACAO, e.VALORAULAS, (e.VALORAULAS * 0.95) as aulasdesconto, e.VALORMATERIAL, (e.VALORMATERIAL / 2) as materialparcela, 
        e.QTDEPARCELASAULAS as parcelas, (e.VALORAULAS / e.QTDEPARCELASAULAS) as valorparcela,
        d.descontoMaior, d.descontoMenor, d.diaLimiteDescontoMaior, d.diaLimiteDescontoMenor, e.MATERIALESCOLA as materialDaEscola,
        e.mesini, e.anoini, e.mesfim, e.anofim,
        CASE when DATEDIFF(YEAR, a.NASCIMENTO, CURRENT_DATE) >= 18 then
        	a.NOME
        else
        	b.NOME
        end as CONTRATANTE
from aluno a
left join RESPONSAVEL b on a.ID = b.ALUNO_ID
inner join nivel c on a.NIVELATUAL_ID = c.ID
inner join matricula e on a.ID = e.ALUNO_ID and c.ID = e.NIVEL_ID
inner join config d on 1 = 1
where a.ID = $P{AlunoID}
AND a.ATIVO = 1]]>
	</queryString>
