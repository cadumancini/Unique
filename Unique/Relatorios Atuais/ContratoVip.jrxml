	<parameter name="AlunoID" class="java.lang.Long">
		<parameterDescription><![CDATA[]]></parameterDescription>
	</parameter>
	<queryString language="SQL">
		<![CDATA[select a.ID, a.NOME, a.ENDERECO, a.NUMERO, a.BAIRRO, c.DESCRICAO,
        a.CIDADE, a.ESTADO, a.CEP, a.EMAIL, a.TELEFONE, a.CELULAR, (e.VALORMENSAL * E.QTDEPARCELASAULAS) AS VALORTOTAL,
        ((e.VALORMENSAL * QTDEPARCELASAULAS) * 0.95) AS VALORDESC,
        a.CPF, a.RG, b.NOME as respNome, b.CPF as respCpf, b.RG as respRg, b.TELEFONE as respTel, b.CELULAR as respCelular,
        c.CODIGO, e.VALORMENSAL, e.VALORMATERIAL, (e.VALORMATERIAL / 2) as materialparcela, e.VALORAULAEXCEDENTE,
        d.descontoMaior, d.descontoMenor, d.diaLimiteDescontoMaior, d.diaLimiteDescontoMenor, e.MATERIALESCOLA as materialDaEscola,
        E.AULASSEMANAIS, TRUNC((E.MINUTOSSEMANAIS / 60), 0) AS HORASSEMANAIS, (E.MINUTOSSEMANAIS - (TRUNC((E.MINUTOSSEMANAIS / 60), 0) * 60)) AS MINUTOSSEMANAIS,
        E.MESINI, E.ANOINI, E.MESFIM, E.ANOFIM, E.QTDEPARCELASAULAS,
        (E.AULASSEMANAIS * TRUNC((E.MINUTOSSEMANAIS / 60), 0) * 42) AS HORAS_MIN,
        (E.AULASSEMANAIS * (E.MINUTOSSEMANAIS - (TRUNC((E.MINUTOSSEMANAIS / 60), 0) * 60)) * 42) AS MINUTOS_MIN,
        (E.AULASSEMANAIS * TRUNC((E.MINUTOSSEMANAIS / 60), 0) * 44) AS HORAS_MAX,
        (E.AULASSEMANAIS * (E.MINUTOSSEMANAIS - (TRUNC((E.MINUTOSSEMANAIS / 60), 0) * 60)) * 44) AS MINUTOS_MAX,
        C.CARGAHORMAX AS CARGAMAX, C.CARGAHORMIN AS CARGAMIN, E.DIAVENCTOBOLETO
from aluno a
left join RESPONSAVEL b on a.ID = b.ALUNO_ID
inner join nivel c on a.NIVELATUAL_ID = c.ID
inner join matriculaVip e on a.ID = e.ALUNO_ID and c.ID = e.NIVEL_ID
inner join config d on 1 = 1
WHERE a.ID = $P{AlunoID}]]>
	</queryString>
